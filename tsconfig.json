{
    "compilerOptions": {
      "jsx": "react",
      "esModuleInterop": true,
      "module": "commonjs",
      "target": "es5",
      "lib": ["es6", "dom"],
      "typeRoots": ["./src/@types", "./node_modules/@types"],
      "baseUrl": "./",
      "paths": {
        "~/*": ["src/*"],
        "@turn/styled": ["src/styled"]
      }
      
    },
    "include": [
      "src",
      "src/declaration.d.ts",
    ]
}